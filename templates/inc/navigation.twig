
{% set menu = _context['menu' ~ menuName | capitalize ] %}
<div class="navigation--{{ menuName }}">
  <ul class="navigation--{{ menuName }}__list">
    {% for item in  menu.get_items %}
      <li class="navigation--{{ menuName }}__item {{item.classes | join(' ')}}"><a class="navigation--primary__link" href="{{item.get_link}}">{{item.title}}</a>
        {% if item.get_children %}
          <ul class="navigation--{{ menuName }}__sub-list">
            {% for child in item.get_children %}
              <li class="navigation--{{ menuName }}__sub--item"><a href="{{child.get_link}}">{{child.title}}</a></li>
            {% endfor %}
          </ul>
        {% endif %}
      </li>
    {% endfor %}
  </ul>
</div>