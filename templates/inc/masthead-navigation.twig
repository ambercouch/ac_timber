<nav id="primaryNavigation" class="masthead__navigation" role="navigation" data-state="off">
  <div class="navigation--cta">
    <ul class="navigation--cta__list">
      {% for item in menuCta.get_items %}
        <li class="navigation--cta__item {{item.classes | join(' ')}}"><a class="navigation--cta__link" href="{{item.get_link}}">{{item.title}}</a>
          {% if item.get_children %}
            <ul class="navigation--cta__sub-list">
              {% for child in item.get_children %}
                <li class="navigation--cta__sub--item"><a href="{{child.get_link}}">{{child.title}}</a></li>
              {% endfor %}
            </ul>
          {% endif %}
        </li>
      {% endfor %}
    </ul>
  </div>
  <div class="navigation--primary">
    <ul class="navigation--primary__list">
      {% for item in menuPrimary.get_items %}
        <li class="navigation--primary__item {{item.classes | join(' ')}}"><a class="navigation--primary__link" href="{{item.get_link}}">{{item.title}}</a>
          {% if item.get_children %}
            <ul class="navigation--primary__sub-list">
              {% for child in item.get_children %}
                <li class="navigation--primary__sub--item"><a href="{{child.get_link}}">{{child.title}}</a></li>
              {% endfor %}
            </ul>
          {% endif %}
        </li>
      {% endfor %}
    </ul>
  </div>
</nav>