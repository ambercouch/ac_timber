{# This is an example of a twig template.
 it can be copied to your theme template folder if you are using the timber plugin #}
<div class="l-post-thumb-list--vacancy">
  <ul class="l-post-thumb-list__list--vacancy">
    {% for post in posts %}
      {% set templateHeaderTitle =  post.title %}
      <li class="l-post-thumb-list__item--vacancy">
        {% include ["inc/vacancy-thumb.twig"] with {'postType' : 'vacancy', 'templateHeaderTitle' : templateHeaderTitle} %}
      </li>
    {% endfor %}
  </ul>
</div>
