              new Date().getTime(),event:'gtm.js'});var f=d.getElementsByTagName(s)[0],
          j=d.createElement(s),dl=l!='dataLayer'?'&l='+l:'';j.async=true;j.src=
          'https://www.googletagmanager.com/gtm.js?id='+i+dl;f.parentNode.insertBefore(j,f);
      })(window,document,'script','dataLayer','GTM-NTCQ5JC');</script>
  <!-- End Google Tag Manager -->

  <!-- Facebook Pixel Code -->
  <script>
      !function(f,b,e,v,n,t,s)
      {if(f.fbq)return;n=f.fbq=function(){n.callMethod?
          n.callMethod.apply(n,arguments):n.queue.push(arguments)};
          if(!f._fbq)f._fbq=n;n.push=n;n.loaded=!0;n.version='2.0';
          n.queue=[];t=b.createElement(e);t.async=!0;
          t.src=v;s=b.getElementsByTagName(e)[0];
          s.parentNode.insertBefore(t,s)}(window, document,'script',
          'https://connect.facebook.net/en_US/fbevents.js');
      fbq('init', '2672348146331329');
      fbq('track', 'PageView');
  </script>
  <noscript><img height="1" width="1" style="display:none"
                 src="https://www.facebook.com/tr?id=2672348146331329&ev=PageView&noscript=1"
    /></noscript>
  <!-- End Facebook Pixel Code -->

  <meta charset="{{ site.charset }}">
  <meta http-equiv="x-ua-compatible" content="ie=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <link rel="profile" href="http://gmpg.org/xfn/11">
  <link rel="pingback" href="{{ site.pingback_url }}">


  {% include 'inc/favicon.twig' %}

  <!-- Critical CSS -->
  <style>
    {% include 'critical.css' ignore missing %}
  </style>

  {% include 'inc/dynamic-styles.twig' %}

  {{ fn('wp_head') }}

  {% block after_head %}
  {% endblock %}

  <!-- Global site tag (gtag.js) - Google Ads: 703172949 -->
  <script async src="https://www.googletagmanager.com/gtag/js?id=AW-703172949"></script>
  <script>
      window.dataLayer = window.dataLayer || [];
      function gtag(){dataLayer.push(arguments);}
      gtag('js', new Date());

      gtag('config', 'AW-703172949');
  </script>


  <script>
      gtag('config', 'AW-703172949/M62dCMr_mrQBENWips8C', {
          'phone_conversion_number': '0800 029 1991'
      });
  </script>

</head>
<!-- BS Deploy -->
<body id="siteBody" class="{{ body_class }} {{ hasHeroClass }} no-js"  data-mobile="off" data-window-top="true" {{ fn('_act_body_data') }}>

<!-- Google Tag Manager (noscript) -->
<noscript><iframe src="https://www.googletagmanager.com/ns.html?id=GTM-NTCQ5JC"
                  height="0" width="0" style="display:none;visibility:hidden"></iframe></noscript>
<!-- End Google Tag Manager (noscript) -->

<div class="defs" style="display: none">
  {% include 'inc/defs.svg' ignore missing %}
</div>

<div id="page" class="site remodal-bg">

  {% include 'site-accessibility.twig' %}

  {% block header %}
    <header id="masthead" class="site__masthead{{ siteMastheadModClass }} {{ hidePageMasthead ? 'is-hidden' : 'is-not-hidden' }}" role="banner">
      {% include 'inc/masthead.twig' %}
    </header>
  {% endblock %}

  {% block mobile_nav %}
    {% include 'inc/mobile-nav.twig' %}
  {% endblock %}

  {% block hero %}
    <section id="siteHero" class="site__hero">
      {% include 'inc/hero.twig' ignore missing %}
    </section>
  {% endblock %}

  {% block before_main %}
  {% endblock %}

  <section id="primary" class="site__main">
    <div class="c-main{{ main_mod }} {% if hasSideBar == true %}has-side-bar{% else %}has-no-side-bar{% endif %}">
      <main class="c-main__content{{ main_mod }}">
        {% block before_content %}
        {% endblock %}
        <div class="content{{ content_mod }}">
          {% block contentHeader %}
          {% endblock %}
          {% block content %}
            <p>Hello world! This AC Timber. I am the content Block in base.twig. Extend me!</p>
          {% endblock %}
          {% block contentFooter %}
          {% endblock %}
        </div>
        {% block belowContent %}
          {% if (fn('comments_open')) %}
            {% include 'inc/comments.twig' %}
          {% endif %}
        {% endblock %}
      </main>
      {% block sidebar %}
      {% endblock %}
    </div>
  </section>

  {% block after_main %}
  {% endblock %}

  {% block subsidiary %}
    {% if subsidiary_widgets or menuService %}
      <section id="subsidiary" class="site__subsidiary">
          {% include 'inc/subsidiary.twig' %}
      </section>
    {% endif %}
  {% endblock %}

  {% block footer %}
    <footer class="site__footer" role="contentinfo">
      {% include 'inc/footer.twig' %}
    </footer>
  {% endblock %}

  {% include 'inc/footer_scripts.twig' %}

</div>{# /#page #}

</body>

</html>
