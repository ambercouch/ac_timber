<!doctype html>
<html {{ site.language_attributes }} data-env-{{environment}}={{environment}}>
<head>
  <!-- base.twig -->
  {% block before_head %}
    <!-- beforehead block -->
  {% endblock %}

  <!-- Google Tag Manager -->
